{
  "name": "3rp.io",
  "version": "0.0.3",
  "author": {
    "name": "Michael Reyes",
    "email": "mreyes@webassemblyio.com",
    "url": "www.webassemblyio.com"
  },
  "description": "Angular 17 - Azle",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "TypeScript",
    "Angular",
    "Canister"
  ],
  "scripts": {
    "build": "",
    "prebuild": "",
    "prestart": "",
    "init:dfx": " npm run restart:clean:dfx && npm run refresh:api && dfx stop",
    "start": "rm -rf .dfx && npx kill-port 8002 && npx kill-port 4200 && npm run restart:clean:dfx && npm run refresh:api && npm run refresh:ii && npm run generate:api && npm run serve:both && npm run print:ng",
    "test": "npm run restart:dfx && ng test",
    "dfx:local": "npm run restart:clean:dfx && npm run refresh:api && npm run generate:api ",
    "deploy:dfx": "npm run restart:dfx && npm run build:prod && dfx deploy --network ic --no-wallet",
    "serve:both": "concurrently -k \"npm run watch:api\" \"ng serve && dfx stop\"",
    "watch:api": "nodemon --watch src --ext mo -x \"npm run refresh:api\"",
    "print:dfx:www": "echo local IC server: http://127.0.0.1:8000/?canisterId=$(dfx canister id www)",
    "print:dfx:api": "echo local IC server: http://127.0.0.1:8000/?canisterId=$(dfx canister id api)",
    "generate:api": "dfx generate api",
    "restart:dfx": "dfx stop && dfx start --background",
    "restart:clean:dfx": "dfx stop && dfx start --background --clean",
    "refresh:api": "dfx deploy api && npm run copy:dfx",
    "refresh:ii": "dfx deploy internet_identity && npm run copy:dfx",
    "copy:dfx": "rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/** --exclude='assets/' --exclude='idl/' --exclude='*.wasm' --exclude='*.most' --exclude='*.ts' --delete src/declarations",
    "build:prod": "export NODE_ENV=production && ng build --configuration production",
    "build:dev": "export NODE_ENV=development && ng build --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "~17.0.0",
